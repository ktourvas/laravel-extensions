{
  "name": "ktourvas/laravel-extensions",
  "type": "collection of laravel based functionality extending classes",
  "require": {
    "laravel/framework": "^5.4"
  },
  "authors": [
    {
      "name": "Kostas Tourvas",
      "email": "k.tourvas@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Laravel\\extensions\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Laravel\\extensions\\LaravelExtensionsServiceProvider"
      ]
    }
  }
}
